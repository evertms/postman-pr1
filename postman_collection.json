		{
			"name": "Create user",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"Status code is 201\", function () {\r",
							"    pm.response.to.have.status(201);\r",
							"});\r",
							"\r",
							"pm.test(\"Response has the field 'id'\", function () {\r",
							"    var jsonData = pm.response.json();\r",
							"    pm.expect(jsonData).to.have.property('id');\r",
							"});\r",
							"\r",
							"pm.environment.set(\"user_id\", pm.response.json().id);\r",
							"pm.environment.set(\"user_name\", pm.response.json().name);\r",
							"var responseData = pm.response.json();\r",
							"var requestData = JSON.parse(pm.request.body.raw);\r",
							"\r",
							"var requestName = requestData.name;\r",
							"var responseName = responseData.name;\r",
							"\r",
							"pm.test(\"response 'name' is the same than the given\", function () {\r",
							"    pm.expect(responseName).to.eql(requestName);\r",
							"});"
						],
